<?xml version="1.0" encoding="ISO-8859-1"?>


<job  order="yes" stop_on_error="no" title="Purge du cache forcé en rm">
    <params >
        <include  live_file="../arii.params.xml"/>
    </params>

    <script  language="shell">
        <![CDATA[
echo === ${SCHEDULER_JOB_NAME} ============================================
cd $SCHEDULER_PARAM_ARII
rm -rf app/cache/*
        ]]>
    </script>

    <run_time />
</job>
