<link rel="stylesheet" type="text/css" href="{{ asset('dhtmlx/skins/terrace/dhtmlx.css') }}" />
<script src="{{ asset('dhtmlx/codebase/dhtmlx.js') }}" type="text/javascript"></script>
{% endblock dhtmlx %}
{% block dhtmlx_plus %}
{% endblock dhtmlx_plus %}
{% block body %}
{% endblock body %}
{% block script %}
<script>
function StateRibbon (itemid,state) {
    switch(itemid) {
        default:
            alert(itemid);
    }
}

function ClickRibbon (itemid,state) {
    switch(itemid) {
        case 'home':
            window.location = "{{ url('arii_Home_index') }}";
            break;
        case 'profile':
            window.location = "{{ url('arii_my_account') }}";
            break;
        case 'filter':
            window.location = "{{ url('arii_filters') }}";
            break;
        case 'audit':
            window.location = "{{ url('arii_Home_audit') }}";
            break;
        case 'errors':
            window.location = "{{ url('arii_Home_errors') }}";
            break;
        case 'docs':
            window.location = "{{ url('arii_docs') }}";
            break;
        case 'my_account':
            break;
        default:
            alert(itemid);
    }
}
</script>
{% endblock script %}