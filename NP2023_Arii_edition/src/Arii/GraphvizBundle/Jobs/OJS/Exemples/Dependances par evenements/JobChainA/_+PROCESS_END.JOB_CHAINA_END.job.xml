<?xml version="1.0" encoding="ISO-8859-1"?>


<job  title="Submit Events" order="yes" stop_on_error="no">
    <description >
        <include  file="jobs/JobSchedulerSubmitEventJob.xml"/>
    </description>

    <params >
        <param  name="scheduler_event_action" value="add"/>

        <param  name="scheduler_event_class" value="PROCESS_END"/>

        <param  name="scheduler_event_id" value="JOB_CHAINA_END"/>

        <param  name="scheduler_event_expires" value="never"/>
    </params>

    <script  language="java" java_class="sos.scheduler.job.JobSchedulerSubmitEventJob"/>

    <run_time />
</job>
