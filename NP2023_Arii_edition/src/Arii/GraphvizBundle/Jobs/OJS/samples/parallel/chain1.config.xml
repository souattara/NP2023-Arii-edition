<?xml version="1.0" encoding="ISO-8859-1"?>

<?xml-stylesheet type="text/xsl" href="scheduler_configuration_documentation.xsl"?> 
<settings>
    <job_chain name="chain1">
        <note language="de"/>

        <note language="en"/>

        <order>
            <params>
                <param name="exe_jobchain"
                       value="/samples/parallel/EXECUTER_chain1"/>

                <note language="de">
                    <div xmlns="http://www.w3.org/1999/xhtml"/>
                </note>

                <note language="en"/>
            </params>

            <process state="success">
                <note language="de"/>

                <note language="en"/>

                <params/>
            </process>

            <process state="A11">
                <note language="de"/>

                <note language="en"/>

                <params/>
            </process>

            <process state="B1">
                <note language="de"/>

                <note language="en"/>

                <params/>
            </process>

            <process state="testjob1">
                <note language="de"/>

                <note language="en"/>

                <params>
                    <param name="sync_command"
                           value="dir"/>

                    <note language="de">
                        <div xmlns="http://www.w3.org/1999/xhtml"/>
                    </note>

                    <note language="en"/>
                </params>
            </process>

            <process state="testjob2">
                <note language="de"/>

                <note language="en"/>

                <params>
                    <param name="sync_command"
                           value="ping -n 20 localhost"/>

                    <note language="de">
                        <div xmlns="http://www.w3.org/1999/xhtml"/>
                    </note>

                    <note language="en"/>
                </params>
            </process>
        </order>
    </job_chain>
</settings>
